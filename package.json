{
  "name": "harbormaster",
  "version": "0.0.0",
  "description": "docker ui, master of docks and controller of containers",
  "main": "./server.js",
  "browser": "./client.js",
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ]
    ]
  },
  "dependencies": {
    "react": "~15.4.2",
    "react-async": "~2.1.0",
    "react-router-component": "~0.36.3",
    "koa": "^1.2.4",
    "koa-compress": "^1.0.4",
    "koa-response-time": "^1.0.2",
    "koa-mount": "^1.2.5",
    "koa-session-redis": "^1.1.0-rc3",
    "koa-static": "^2.0.0",
    "superagent": "~3.3.1",
    "fibers": "~1.0.1",
    "node-jsx": "~0.13.3",
    "function-proxy": "~0.5.1",
    "dockerode": "^2.3.1",
    "callback-count": "~0.2.0",
    "bind-right": "~0.6.0",
    "browserify": "~13.3.0",
    "keypather": "^2.0.1",
    "http-proxy": "^1.1.1"
  },
  "devDependencies": {
    "reactify": "~1.1.1",
    "envify": "~4.0.0",
    "browserify": "~13.3.0",
    "connect-browserify": "~4.0.0",
    "uglify-js": "~2.7.5",
    "supervisor": "~0.12.0"
  },
  "scripts": {
    "test": "NODE_LIB=./lib mocha",
    "watch": "supervisor --harmony -n -i node_modules -e js,jsx server.js",
    "build": "NODE_ENV=production browserify --dg ./ | uglifyjs -cm 2>/dev/null > ./assets/bundle.js",
    "start": "NODE_ENV=production node --harmony server.js",
    "clean": "rm -f ./assets/bundle.js"
  },
  "author": "",
  "license": "MIT"
}
